{
    "modbus-serial": {
        "device": "/dev/ttyUSB0",
        "options": {
            "baudRate": 115200
        }
    },
    "statistics": {
	"disabled": false,
	"timeslotSeconds": 60,
	"tmpFile": "/var/log/fronius/.tmpnibe1155",
        "dbtyp": "csvfile",
	"csvfile": {
            "filename": "/var/log/fronius/nibe1155-data_%Y-%M-%D.csv"
        }
    },
    "server": {
        "start": true,
        "port": 8080,
        "morgan": {
            "disabled": true,
            "config": "tiny"
         },
         "pin": "1234"
    },
    "debug": {
        "depth": 3,
        "colors" : true,
        "wtimediff": 6,
        "time": "ddd, yyyy-mm-dd HH:MM:ss.l",
        "wmodule": 16,
        "wlevel": 6,
        "location": "-*",
        "enabled": "*::WARN, *::INFO, *ModbusSerial::INFO, HeatPump.State::INFO, HeatPump.Action::*, *Nibe1155::INFO, *ModeEconomy::*"
    }

}

